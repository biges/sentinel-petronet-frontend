mport { DEVICE_TYPES } from \"@/constant\";\nimport { mapActions } from \"vuex\";\nexport default {\n  name: \"DeviceCard\",\n  data() {\n    return {\n      radio: null,\n      model: null,\n      brand: null,\n    };\n  },\n  props: {\n    data: {\n      default: {},\n    },\n  },\n  watch: {\n    data: function (val) {\n      console.log(val);\n    },\n  },\n  computed: {},\n  methods: {\n    ...mapActions({\n      getDeviceModelId: \"model/getDeviceModelId\",\n    }),\n    getHardwareType(val) {\n      return [...DEVICE_TYPES].find((t) => {\n        return t.key == val;\n      });\n    },\n    handleCardClick(device_id) {\n      console.log(\"Click !!\", device_id);\n      this.$emit(\"selectedDevice\", device_id);\n    },\n  },\n  created() {\n    let data = null;\n    data = this.getDeviceModelId(2);\n    data.then((r) => {\n      this.model = r.data.data.device_model;\n      this.brand = r.data.data.device_model.device_brand;\n    });\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"@/assets/scss/style.scss\";\n\n.device-card {\n  display: flex;\n  flex-direction: column;\n  justify-content: spa;\n  background: $hybrone_background_color;\n  border: 0.5px solid $hybrone_border_color;\n  box-sizing: border-box;\n  border-radius: 10px;\n  margin: 33px;\n  margin-bottom: 20px;\n  min-width: 394px;\n  min-height: 150px;\n\n  &:hover {\n    border: 0.5px solid $hybrone_light_blue;\n    box-shadow: 40px $hybrone_light_blue;\n  }\n  //   &:active {\n  //     border: 0.5px solid $hybrone_light_blue;\n  //   }\n  .left {\n    margin-left: 23px;\n    margin-top: 36px;\n    span {\n      width: 100%;\n      height: 23px;\n      font-family: Roboto, sans-serif;\n      font-style: normal;\n      font-weight: 300;\n      font-size: 14px;\n      line-height: 16px;\n      display: flex;\n      align-items: center;\n      color: #000000;\n      p {\n        display: flex;\n        align-items: flex-start;\n        width: 50%;\n        font-family: Roboto, sans-serif;\n        font-style: normal;\n        font-weight: 500;\n        font-size: 14px;\n        line-height: 16px;\n        margin-left: 25px;\n\n        color: #000000;\n      }\n    }\n  }\n  .right {\n    display: flex;\n    justify-content: flex-end;\n    padding-top: 12px;\n    width: 100%;\n    span {\n      font-family: Roboto, sans-serif;\n      font-style: normal;\n      font-weight: 500;\n      font-size: 14px;\n      line-height: 16px;\n\n      text-align: right;\n      margin-right: 20px;\n      margin-top: 12px;\n      /* Gray Dark */\n\n      color: #444444;\n    }\n  }\n}\n</style>\n"]}]}