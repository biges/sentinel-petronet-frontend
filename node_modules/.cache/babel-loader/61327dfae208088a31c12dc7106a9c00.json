AAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,GAAA,CAAA,GAAA,EAAA,GAAA,CAAA,IAAA,CAAA,EAAA,EAAA;;AACA,gBAAA,KAAA,CAAA,YAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA,KATA;AAUA,IAAA,6BAVA,2CAUA;AACA,WAAA,iBAAA,CAAA,KAAA,kBAAA,CAAA,QAAA;AACA;AAZA,IAxBA;AAsCA,EAAA,OAtCA,qBAsCA;AAAA;;AACA,QAAA,CAAA,GAAA,OAAA;AACA,SAAA,IAAA,GAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,OAAA,CAAA,CAAA,SAAA,EAAA,SAAA,CAAA,EAAA,CAAA,CAAA;AACA,IAAA,CAAA,CAAA,SAAA,CAAA,KAAA,IAAA,EAAA;AACA,MAAA,OAAA,EAAA,EADA;AAEA,MAAA,WAAA,EACA,iKAHA;AAIA,MAAA,EAAA,EAAA;AAJA,KAAA,EAKA,KALA,CAKA,KAAA,IALA;;AAMA,QAAA,KAAA,MAAA,CAAA,IAAA,IAAA,UAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,4BAAA,EAAA,KAAA,kBAAA;AACA,WAAA,iBAAA,CAAA,KAAA,kBAAA,CAAA,QAAA;AACA,KAZA,CAaA;;;AACA,QAAA,CAAA,eAAA,EAAA,eAAA,EAAA,QAAA,CAAA,KAAA,MAAA,CAAA,IAAA,CAAA,EAAA;AACA,WAAA,IAAA,CAAA,EAAA,CAAA,OAAA,EAAA,UAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,EAAA,MAAA,CAAA,IAAA,CAAA,WAAA,CAAA,MAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,CAAA,CAAA,MAAA,CAAA,CAAA,CAAA,MAAA,EAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA;;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,qBAAA,oBACA,MAAA,CAAA,MAAA,CAAA,SAAA,EADA;;AAGA,QAAA,MAAA,CAAA,iBAAA,GAAA,IAAA;AACA,OAPA;AAQA;;AAEA,IAAA,GAAA,CAAA,GAAA,CAAA,2BAAA,EAAA;AAAA,aACA,MAAA,CAAA,6BAAA,EADA;AAAA,KAAA;AAGA;AAlEA,CAAA","sourcesContent":["<template>\n  <div>\n    <div v-if=\"this.$route.name === 'Map'\" class=\"legand-map\">\n      <spam class=\"legand-text\">\n        <i class=\"normal\"></i>\n        Normal\n      </spam>\n      <spam class=\"legand-text\">\n        <i class=\"non-device\"></i>\n        Cihaz Yok\n      </spam>\n      <spam class=\"legand-text\"> <i class=\"fault\"></i> HatalÄ± </spam>\n    </div>\n    <div id=\"map\" :style=\"this.$route.name !== 'Map' ? 'margin:0' : ''\"></div>\n  </div>\n</template>\n\n<script>\nimport leaflet from \"leaflet\";\nimport { mapGetters } from \"vuex\";\nimport { mapActions } from \"vuex\";\nimport { bus } from \"@/main.js\";\nexport default {\n  name: \"MapView\",\n  data() {\n    return {\n      maps: null,\n      marker: null,\n      isRender: true,\n      bus: {},\n    };\n  },\n  props: {\n    premise: {\n      type: Object,\n      default: () => {\n        return {};\n      },\n    },\n  },\n  watch: {},\n  computed: {\n    ...mapGetters({\n      getCurrentLocation: \"getCurrentLocation\",\n    }),\n  },\n  methods: {\n    ...mapActions({\n      setSelectedLocation: \"setSelectedLocation\",\n    }),\n    async addMarkerAndFlyTo(val) {\n      console.log(\"Val\", val);\n      leaflet.marker([val.lat, val.long]).addTo(this.maps);\n      this.maps.flyTo([val.lat, val.long], 14);\n      this.$forceUpdate();\n    },\n    handleCurrentChangeRowPremise() {\n      this.addMarkerAndFlyTo(this.getCurrentLocation.location);\n    },\n  },\n  mounted() {\n    let L = leaflet;\n    this.maps = leaflet.map(\"map\").setView([38.963745, 35.243322], 6);\n    L.tileLayer(this.$map, {\n      maxZoom: 18,\n      attribution:\n        'Map data &copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a> contributors, <a href=\"https://creativecommons.org/licenses/by-sa/2.0/\">CC-BY-SA</a>',\n      id: \"base\",\n    }).addTo(this.maps);\n    if (this.$route.name == \"Premises\") {\n      console.log(\"GetCurrentLocation Premise\", this.getCurrentLocation);\n      this.addMarkerAndFlyTo(this.getCurrentLocation.location);\n    }\n    // if (this.$route.name == \"CreatePremise\") {\n    if ([\"CreatePremise\", \"UpdatePremise\"].includes(this.$route.name)) {\n      this.maps.on(\"click\", (e) => {\n        if (this.marker) this.maps.removeLayer(this.marker);\n        this.marker = L.marker(e.latlng).addTo(this.maps);\n        this.$store.dispatch(\"setSelectedLocation\", {\n          ...this.marker.getLatLng(),\n        });\n        this.selected_location = true;\n      });\n    }\n\n    bus.$on(\"onCurrentChangeRowPremise\", () =>\n      this.handleCurrentChangeRowPremise()\n    );\n  },\n};\n</script>\n<style lang=\"scss\">\n#map {\n  width: 100%;\n  height: 100vh;\n  margin-left: 14px;\n}\n/*Lejant Scss */\n.legand-map {\n  position: absolute;\n  display: grid;\n  grid-template-rows: repeat(3, 1fr);\n  left: 90%;\n  top: 87%;\n  width: 150px;\n  height: 100px;\n  z-index: 9999;\n  opacity: 0.5;\n}\n.legand-text {\n  font-family: Roboto, sans-serif;\n  font-style: normal;\n  font-weight: normal;\n  font-size: 18px;\n  line-height: 21px;\n  display: flex;\n  align-items: center;\n}\n.normal {\n  background-color: #6fcf97;\n  width: 20px;\n  height: 20px;\n  border-radius: 10px;\n  margin-right: 7px;\n}\n.non-device {\n  background-color: #f2994a;\n  width: 20px;\n  height: 20px;\n  border-radius: 10px;\n  margin-right: 7px;\n}\n.fault {\n  background-color: #eb5757;\n  width: 20px;\n  height: 20px;\n  border-radius: 10px;\n  margin-right: 7px;\n}\n</style>\n"],"sourceRoot":"src/components/map"}]}