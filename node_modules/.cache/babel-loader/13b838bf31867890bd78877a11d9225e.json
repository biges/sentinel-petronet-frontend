    indicators: {},\n    };\n  },\n  components: {\n    DashboardDonutChart,\n  },\n  created() {\n    this.indicators = { ...DASHBOARD_VARIABLES[\"HAP\"] };\n    let dashboard_data = this.$api({\n      ...endpoints.getDashboardData,\n    }).then((r) => {\n      this.indicators[0].state.series.total = r.data.data.report.total_device;\n      this.indicators[0].state.series.online = r.data.data.report.online;\n      this.indicators[0].state.series.offline = r.data.data.report.offline;\n      this.indicators[1].state.series.normal = r.data.data.report.normal;\n      this.indicators[1].state.series.alarm = r.data.data.report.alarmed;\n      this.indicators[2].state.series.arm = r.data.data.report.armed;\n      this.indicators[2].state.series.disarm = r.data.data.report.dis_armed;\n      this.indicators[3].state.series.normal = r.data.data.report.normal;\n      this.indicators[3].state.series.low_charge = r.data.data.report.no_energy;\n      this.indicators[4].state.series.normal =\n        r.data.data.report.battery_normal;\n      this.indicators[4].state.series.low = r.data.data.report.battery_low;\n      this.indicators[5].state.series.normal = r.data.data.report.normal;\n      this.indicators[5].state.series.sabotage = r.data.data.report.sabotage;\n      this.indicators[5].state.series.fault = r.data.data.report.trouble;\n    });\n    console.log(dashboard_data);\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"@/assets/scss/style.scss\";\n\n.content {\n  max-width: 100%;\n  height: 100%;\n  background-color: $hybrone_background_color;\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  align-content: space-evenly;\n  justify-content: space-between;\n}\n@media screen and (min-width: 1500px) {\n  .content {\n    display: grid;\n    grid-template-rows: repeat(2, auto);\n    grid-template-columns: repeat(3, auto);\n  }\n}\n</style>\n"],"sourceRoot":"src/components/dashboard"}]}